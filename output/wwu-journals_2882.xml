                    <titleInfo lang="ger">
                        <nonSort>Der </nonSort>
                        <title>Lorbeerkranz mit Tänie</title>
                        <subTitle>Herkunft und Genese einer kaiserlichen Insignie</subTitle>
                    </titleInfo>
                    <titleInfo lang="eng" type="translated">
                        <nonSort>The </nonSort>
                        <title>laurel wreath with tainia</title>
                        <subTitle>Origin and genesis of an imperial insignia</subTitle>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Biedermann, David</displayForm>
                        <namePart type="family">Biedermann</namePart>
                        <namePart type="given">David</namePart>
