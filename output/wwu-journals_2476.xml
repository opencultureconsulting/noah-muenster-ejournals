                    <titleInfo lang="ger">
                        <title>Münzversorgung von Haltern und Kalkriese – eine Diskussion</title>
                    </titleInfo>
                    <titleInfo lang="eng" type="translated">
                        <nonSort>The </nonSort>
                        <title>supply of coins to Haltern and Kalkriese - a discussion</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Werz, Ulrich</displayForm>
                        <namePart type="family">Werz</namePart>
                        <namePart type="given">Ulrich</namePart>
