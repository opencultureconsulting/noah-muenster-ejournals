                    <titleInfo lang="ger">
                        <title>Association of acute disseminated encephalomyelitis (ADEM) and COVID-19 in a pediatric patient</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Chen, Liam</displayForm>
                        <namePart type="family">Chen</namePart>
                        <namePart type="given">Liam</namePart>
                        <role>
